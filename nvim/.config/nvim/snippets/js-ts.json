{
  "console log":{
    "prefix":"cl",
    "body": [
      "console.log(`$1`);"
    ]
  },
  "for loop":{
    "prefix":"fo",
    "body":[
      "for (let i=${1:0}; i < ${2:condition}; ${3:i++}) {",
      "$4",
      "}"
    ]
  },
  "arrow function":{
    "prefix":"af",
    "body":[
      "const ${1:functionName} = $2 ($3) => {",
      "  $0",
      "};"
    ]
  }
}
